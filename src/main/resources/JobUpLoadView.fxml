<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <Separator opacity="0.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <JFXButton fx:id="saveBtn" buttonType="RAISED" style="-fx-border-width: 2; -fx-border-radius: 10; -fx-border-color: #1aae41;" text="Save Job" textFill="#1aae41">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="cancelBtn" buttonType="RAISED" style="-fx-border-width: 2; -fx-border-radius: 10; -fx-border-color: #cd1313;" text="Cancel" textFill="#cd1313">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </JFXButton>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </bottom>
   <center>
      <ScrollPane fx:id="tableViewScrollPane" fitToHeight="true" fitToWidth="true" BorderPane.alignment="TOP_LEFT">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
        </content>
         <BorderPane.margin>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </BorderPane.margin>
      </ScrollPane>
   </center>
</BorderPane>
